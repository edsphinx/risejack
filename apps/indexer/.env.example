# Rise Indexer Configuration
# Copy this to .env and fill in values

# Rise Chain RPC (WebSocket preferred for real-time)
RISE_WSS_URL=wss://testnet.riselabs.xyz/ws
RISE_RPC_URL=https://testnet.riselabs.xyz

# RiseJack contract address
RISEJACK_CONTRACT_ADDRESS=0x...

# Supabase PostgreSQL connection string
# Format: postgresql://user:password@host:port/database
DATABASE_URL=postgresql://postgres:password@db.xxx.supabase.co:5432/postgres
